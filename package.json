{
  "name": "html-table-to-dataframe",
  "version": "1.0.25",
  "description": "Convert HTML tables to data-frames",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky",
    "prepublishOnly": "npm test && npm run lint",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint 'src/**/*.{ts,tsx}' --fix",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "format:check": "prettier --config .prettierrc 'src/**/*.ts' --check",
    "release": "release-it --github.release",
    "release:ci": "npm run release -- --ci --npm.skipChecks --no-git.requireCleanWorkingDir",
    "release:patch": "npm run release -- patch",
    "release:minor": "npm run release -- minor",
    "release:major": "npm run release -- major"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/serialbandicoot/html-table-to-dataframe.git"
  },
  "keywords": [
    "html",
    "table",
    "dataframe",
    "typescript",
    "jsdom",
    "cli-table3",
    "pretty-print"
  ],
  "author": "Sam Treweek <samtreweek@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/serialbandicoot/html-table-to-dataframe/issues"
  },
  "homepage": "https://github.com/serialbandicoot/html-table-to-dataframe#readme",
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^21.1.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "globals": "^15.9.0",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "release-it": "^17.6.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.1.6",
    "typescript-eslint": "^8.2.0"
  },
  "dependencies": {
    "cli-table3": "^0.6.0",
    "jsdom": "^20.0.0"
  }
}
